# Web Certificate Sync Configuration

# Maximum parallel jobs
MAX_JOBS=10

# Remote directory base path (on target servers)
REMOTE_DIR_BASE=/etc/ssl

# Path to servers list file (for testing, we'll use a local path)
SERVER_LIST_PATH=d:/GolandProjects/ssl_update/web_cert_sync/test_servers.txt

# Remote SSH user
REMOTE_USER=root

# ACME certificate root directory
ACME_CERT_ROOT=/root/.acme.sh

# Dry run mode (set to True for testing without actual SSH connections)
DRY_RUN=True
